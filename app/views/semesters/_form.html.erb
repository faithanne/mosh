<%= simple_form_for(@semester, html: { class: 'form-horizontal' }) do |f| %>
  <%= f.input :name %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :advisement_start %>
  <%= f.association :term %>
  <%= f.input :year %>
  <%= f.input :advisement_end %>
  <%= f.button :submit %>
<% end %>
