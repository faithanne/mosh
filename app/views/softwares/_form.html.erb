<%= simple_form_for(@software, html: { class: 'form-horizontal' }) do |f| %>
  <%= f.input :name %>
  <%= f.input :year %>
  <%= f.input :comment %>
  <%= f.input :source_url %>
  <%= f.association :software_category %>
  <%= f.input :version %>
  <%= f.association :silent_install %>
  <%= f.association :software_test %>
  <%= f.button :submit %>
<% end %>
